<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- #3573EE -->
    <SolidColorBrush x:Key="DarkBackground1" Color="#283246"/>
    <SolidColorBrush x:Key="DarkBackground2" Color="#1B1B1B"/>
    <SolidColorBrush x:Key="DarkHighlight1" Color="#5AC2FF"/>
    <SolidColorBrush x:Key="DarkHighlight2" Color="#2D4C89"/>
    <SolidColorBrush x:Key="DarkHighlight3" Color="#EC0000"/>
    <SolidColorBrush x:Key="DarkHighlight4" Color="#B90000"/>

    <SolidColorBrush x:Key="DarkFont1" Color="#F0F5FA"/>
    <SolidColorBrush x:Key="DarkFont2" Color="#A7C9EA"/>
    <SolidColorBrush x:Key="DarkFont3" Color="#70879D"/>

    <LinearGradientBrush x:Key="gradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#A7C9EA" Offset="0"/>
        <GradientStop Color="#283246" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="NaNGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#EC0000" Offset="0"/>
        <GradientStop Color="#B90000" Offset="1.0"/>
    </LinearGradientBrush>

    <DropShadowEffect x:Key="DropShadow" BlurRadius="4" Direction="330" Color="Black" ShadowDepth="5" Opacity="0.5"/>
</ResourceDictionary>